{
  "name": "stock_news_bot",
  "version": "2.0.0",
  "description": "Indian stock tracker with watchlist, portfolio, screener and feed",
  "main": "server.js",
  "scripts": {
    "start": "node --env-file-if-exists=.env server.js",
    "dev": "node --watch --env-file-if-exists=.env server.js",
    "doctor": "node --env-file-if-exists=.env scripts/doctor.js",
    "seed:mongo": "node --env-file-if-exists=.env scripts/seed_mongo_from_json.js",
    "test": "node -e \"console.log('No tests configured yet')\""
  },
  "keywords": [
    "stocks",
    "india",
    "watchlist",
    "portfolio",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.4",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.17.3",
    "mongodb": "^7.1.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2"
  }
}
